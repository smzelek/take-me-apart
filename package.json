    "compression-webpack-plugin": "^9.2.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.4.0",
    "file-loader": "^6.2.0",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-webpack-plugin": "^5.4.0",
    "mini-css-extract-plugin": "^2.6.0",
    "react-snap": "^1.23.0",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.59.1",
